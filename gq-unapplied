#!/bin/bash
#
# Copyright (c) Josef "Jeff" Sipek, 2006
#

source "`dirname $0`/gq.lib"

export GIT_DIR=`find_git_dir`
GQ_DIR="$GIT_DIR/patches"

branch=`get_branch_verify`

n=`wc -l < $GQ_DIR/$branch/status`
n=`expr $n + 1`

idx=0
for p in `cat $GQ_DIR/$branch/series`; do
	idx=`expr $idx + 1`
	[ $idx -lt $n ] && continue

	echo $p
done
